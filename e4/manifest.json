{
    "manifest_version": 3,
    "name": "4E Login Checker",
    "version": "1.0",
    "description": "Memeriksa status login dan menampilkan pesan",
    "permissions": [
        "cookies",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}
